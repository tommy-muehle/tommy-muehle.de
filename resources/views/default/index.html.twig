{% extends 'default.html.twig' %}

{% block title %}Willkommen{% endblock %}

{% block content %}
    <section id="profile" class="item noprint">
        <h2 class="item-title">
            <span class="title">Mein Profil</span>
            <span class="icon-user"></span>
        </h2>

        <div class="item-cont clearfix">
            <div class="hidden">
                <div class="col500 clearfix fl-left">
                    <div class="profile-img"><img src="/images/tommy.jpg" alt="Bild von mir" /></div>
                    <div class="profile-info">
                        <h3>Hallo, ich bin Tommy.</h3>
                        <p>Ich bin Entwickler, Ehemann, ein wenig Nerd mit einer Vorliebe für Technik,
                           Fussball, Filme und gutes Essen.</p>
                    </div>
                </div>

                <div class="col260 fl-right">
                    <ul class="profile-data">
                        <li><h4>Name</h4> <div>Tommy Mühle</div></li>
                        <li><h4>Alter</h4> <div>28</div></li>
                        <li><h4>Address</h4> <div>Hepkestraße 177, 01277 Dresden</div></li>
                        <li><h4>E-mail</h4> <div>tommy.muehle@gmail.com</div></li>
                    </ul>
                </div>
            </div>
            <a class="blog-link" href="https://tommy-muehle.github.io" title="zum Blog">
                <span class="icon"></span>
                <span class="label">BLOG</span>
            </a>
        </div>
        <div id="profile-brd" class="item-border"><span><span></span></span></div>
    </section><!-- /#profile -->

