                                    <div class="skill-data skill6"></div>
                                </div>
                            </aside>

                            <aside class="skill-language">
                                <div class="skill-row clearfix">
                                    <h4 class="skill-title clearfix"><img src="/images/en.png" alt="english" /><span>ENGLISH</span></h4>
                                    <div class="skill-data skill3"></div>
                                </div>
                            </aside>
