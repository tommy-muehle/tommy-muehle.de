                            <div class="skill-row">
                                <h4 class="skill-title">Silex</h4>
                                <div class="skill-data">
                                    <span class="skill-percent-line" data-width="100"></span>
                                    <span class="skill-percent">100%</span>
                                </div>
                            </div>

                            <div class="skill-row">
                                <h4 class="skill-title">Symfony2</h4>
                                <div class="skill-data">
                                    <span class="skill-percent-line" data-width="90"></span>
